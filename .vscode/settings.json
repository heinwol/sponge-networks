{
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "python.analysis.exclude": [
    "submodules",
    ".direnv",
    "result",
    ".nix-env"
  ],
  "mypy.dmypyExecutable": "${workspaceFolder}/.nix-env/bin/dmypy",
  // "mypy.extraArguments": [
  //   // "--exclude",
  //   // "__init__.py",
  // ],
  "python.languageServer": "Default",
  "python.analysis.typeCheckingMode": "basic",
  "python.defaultInterpreterPath": "${workspaceFolder}/.nix-env/bin/python",
}
